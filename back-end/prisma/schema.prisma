// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id          Int     @id @default(autoincrement())
  image       String
  make        String
  model       String
  year        Int
  color       String
  price       Int
  kilometers  Int
}
